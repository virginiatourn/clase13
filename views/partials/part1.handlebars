
    <div class"container">
    {{#if productos}}
    <h2>Productos</h2>
        <table class="table margin-left">
         <thead>
             <tr>
                <th scope="col">Producto</th>
                <th scope="col">Precio</th>
                <th scope="col">imagen</th>
                 <th scope="col">Id</th>
            </tr>
        </thead>
         <tbody>
         {{#each productos }}
            <tr>
            <th class="row">{{title}}</th>
            <td>{{price}}</td>
            <td><img src="{{thumbnail}}" width="50px" height="50px"></td>
            </tr>
        {{/each}}
            </tbody>
    {{else}}
    <p>No hay Productos</p>
{{/if}}
</table>
</div>
  